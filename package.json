{
  "name": "academy-backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "node dist/src/index.js",
    "seed:dev": "ts-node --transpile-only prisma/seed.ts",
    "seed:prod": "node dist/prisma/seed.js",
    "db:seed": "prisma/seed.ts",
    "db:reset": "prisma migrate reset --force",
    "db:fresh": "npm run db:reset && npm run db:seed",
    "lint": "eslint \"src/**/*.{ts,js}\" --ext .ts",
    "lint:fix": "eslint \"src/**/*.{ts,js}\" --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,js,json,md}\"",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:migrate:reset": "prisma migrate reset --force",
    "prisma:db:push": "prisma db push",
    "prisma:db:pull": "prisma db pull"
  },
  "prisma": {
    "seed": "ts-node --transpile-only prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@faker-js/faker": "^9.6.0",
    "@prisma/client": "^5.22.0",
    "@types/jsonwebtoken": "^8.5.9",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "jsonwebtoken": "^8.5.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.19.23",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.4.0",
    "jest": "^29.7.0",
    "nodemon": "^2.0.22",
    "prettier": "^3.6.2",
    "prisma": "^5.22.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.44.1"
  }
}
